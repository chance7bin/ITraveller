<view class="prompt-box data-v-6fd4acb0" hidden="{{isHidden}}"><view class="prompt-content contentFontColor data-v-6fd4acb0"><view class="prompt-title data-v-6fd4acb0">{{title}}</view><view class="prompt-text data-v-6fd4acb0">{{text}}</view><input class="prompt-input data-v-6fd4acb0" type="text" data-event-opts="{{[['input',[['_input',['$event']]]]]}}" value="{{cost}}" bindinput="__e"/><view class="prompt-btn-group data-v-6fd4acb0"><button data-event-opts="{{[['tap',[['_cancel',['$event']]]]]}}" class="btn-item prompt-cancel-btn contentFontColor data-v-6fd4acb0" bindtap="__e">{{btn_cancel}}</button><button data-event-opts="{{[['tap',[['_confirm',['$event']]]]]}}" class="btn-item prompt-certain-btn data-v-6fd4acb0" bindtap="__e">{{btn_certain}}</button></view></view></view>